{
  "name": "veact",
  "version": "1.0.0-beta.1",
  "description": "Mutable state enhancer library for React by @vue/reactivity",
  "keywords": [
    "React mutable state lib",
    "React vue state",
    "React composition API",
    "React state library"
  ],
  "license": "MIT",
  "author": "Surmon",
  "homepage": "https://github.com/veactjs/veact",
  "funding": "https://github.com/veactjs/veact?sponsor=1",
  "repository": "github:veactjs/veact",
  "files": [
    "README.md",
    "CHANGELOG.md",
    "dist",
    "src"
  ],
  "sideEffects": false,
  "type": "module",
  "main": "./dist/veact.umd.cjs",
  "module": "./dist/veact.js",
  "browser": "./dist/veact.umd.cjs",
  "types": "./dist/veact.d.ts",
  "exports": {
    ".": {
      "types": "./dist/veact.d.ts",
      "import": "./dist/veact.js",
      "require": "./dist/veact.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "lint": "eslint src",
    "format": "prettier --write --parser typescript \"src/**/*.ts\"",
    "test": "jest",
    "build": "vite build",
    "ci": "npm run lint && npm run test && npm run build",
    "release": ". ./scripts/release.sh"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17 || ^18",
    "react-dom": "^16.8.0 || ^17 || ^18"
  },
  "dependencies": {
    "@vue/reactivity": "^3.5.0-beta.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.x",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/eslint": "^9.x",
    "@types/jest": "^27.4.0",
    "@types/react": "^18.x",
    "@types/react-dom": "^18.x",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.x",
    "eslint-config-prettier": "^9.x",
    "eslint-plugin-prettier": "^5.x",
    "globals": "^15.9.0",
    "jest": "^29.x",
    "prettier": "^3.x",
    "react": "^18.x",
    "react-dom": "^18.x",
    "ts-jest": "^29.x",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.x",
    "vite": "^5.4.0",
    "vite-plugin-dts": "^4.0.2"
  }
}
